<main class="flex-1 p-8">
   <div class="bg-white shadow rounded-lg p-6 max-w-3xl mx-auto">
      <h3 class="text-xl font-semibold text-gray-700 mb-6">Edit Your Profile</h3>
      <div class="flex items-center space-x-6 mb-6">
         <img src="/placeholder.png" alt="Profile" class="w-24 h-24 rounded-full border">
         <div>
            <h2 class="text-2xl font-bold"><%= user.name %></h2>
            <button class="mt-2 px-4 py-2 bg-yellow-400 text-white rounded-lg hover:bg-yellow-500">Update Image</button>
         </div>
      </div>

      <!-- Profile Form -->
      <form class="space-y-4" action="/profile" method="POST">
         <div>
            <label class="block text-sm font-medium text-gray-600"></label>
            <input name="name" type="text" value="<%= user.name %>"
               class="mt-1 w-full border rounded-lg px-4 py-2 focus:ring focus:ring-yellow-300">
         </div>

         <div>
            <label class="block text-sm font-medium text-gray-600">Email</label>
            <input name="email" type="email" value="<%= user.email  %>"
            disabled
               class="mt-1 w-full border rounded-lg px-4 py-2 focus:ring focus:ring-yellow-300">
         </div>

         <div>
            <label class="block text-sm font-medium text-gray-600">Password Changes</label>
            <input name="currentPassword" type="password" placeholder="Current Password"
               class="mt-1 w-full border rounded-lg px-4 py-2 mb-2 focus:ring focus:ring-yellow-300">
            <input name="newPassword" type="password" placeholder="New Password"
               class="mt-1 w-full border rounded-lg px-4 py-2 mb-2 focus:ring focus:ring-yellow-300">
            <input name="confirmPassword" type="password" placeholder="Confirm New Password"
               class="mt-1 w-full border rounded-lg px-4 py-2 focus:ring focus:ring-yellow-300">
         </div>

         <!-- Buttons -->
         <div class="flex justify-end space-x-4 pt-4">
            <button type="reset"
               class="px-6 py-2 border border-gray-400 rounded-lg text-gray-600 hover:bg-gray-100">Cancel</button>
            <button type="submit" class="px-6 py-2 bg-yellow-400 text-white rounded-lg hover:bg-yellow-500">Save
               Changes</button>
         </div>
      </form>
   </div>
</main>